{
  "name": "social-search-engine-server",
  "version": "1.0.0",
  "description": "API para a aplicação social-search-engine",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint . --fix",
    "lint:report": "./node_modules/.bin/eslint -f json -o ./lint-report.json .",
    "format": "prettier --write \"**/*.+(js|json|yml|yaml|md)\"",
    "start": "node server.js",
    "dev": "NODE_ENV=development nodemon server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "node",
    "api",
    "rest",
    "fastify"
  ],
  "author": "Pablo Maribondo",
  "license": "ISC",
  "dependencies": {
    "fastify": "^2.14.1",
    "got": "^11.1.4",
    "querystring": "^0.2.0",
    "se-scraper": "^1.5.7"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "prettier": "^2.0.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "nodemon": "^2.0.4"
  }
}
